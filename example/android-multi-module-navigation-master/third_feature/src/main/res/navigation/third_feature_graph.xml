<?xml version="1.0" encoding="utf-8"?>
<navigation android:id="@+id/third_feature_graph"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/thirdFeatureFragment">

    <fragment
        android:id="@+id/thirdFeatureFragment"
        android:name="com.jdapplications.third_feature.ThirdFeatureFragment"
        android:label="third_feature_fragment"
        tools:layout="@layout/third_feature_fragment" >
        <action
            android:id="@+id/navigateToSecondScreen"
            app:destination="@id/thirdFeatureFragmentSecondScreen"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/thirdFeatureFragmentSecondScreen"
        android:name="com.jdapplications.third_feature.ThirdFeatureFragmentSecondScreen"
        android:label="ThirdFeatureFragmentSecondScreen"
        tools:layout="@layout/third_feature_fragment_second_screen_fragment" />
</navigation>